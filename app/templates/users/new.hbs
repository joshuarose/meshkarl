<h3>Add new user</h3>
<div class="container">
<form>
  <div class="form-group">
    <label for="name">Name</label>
    {{input type="text" class="form-control" placeholder="name" value=model.name}}
  </div>
  <div class="form-group">
    <label for="email">Email address</label>
    {{input type="email" class="form-control" placeholder="email" value=model.email}}
  </div>
  <button {{action "createUser" model}} type="submit" class="btn btn-default">Submit</button>
</form>
</div>


{{outlet}}
